#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x650d30f31010 .scope module, "ContadorConEcho_tb" "ContadorConEcho_tb" 2 3;
 .timescale -9 -12;
v0x650d30f44420_0 .var "clk", 0 0;
v0x650d30f444f0_0 .var "echo", 0 0;
v0x650d30f445c0_0 .net "echo_duration", 31 0, v0x650d30f442c0_0;  1 drivers
S_0x650d30f311a0 .scope module, "uut" "ContadorConEcho" 2 11, 3 1 0, S_0x650d30f31010;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "echo";
    .port_info 2 /OUTPUT 32 "echo_duration";
v0x650d30ef87b0_0 .net "clk", 0 0, v0x650d30f44420_0;  1 drivers
v0x650d30ef8ba0_0 .var "counter", 31 0;
v0x650d30f44220_0 .net "echo", 0 0, v0x650d30f444f0_0;  1 drivers
v0x650d30f442c0_0 .var "echo_duration", 31 0;
E_0x650d30f2fcc0 .event posedge, v0x650d30ef87b0_0;
    .scope S_0x650d30f311a0;
T_0 ;
    %wait E_0x650d30f2fcc0;
    %load/vec4 v0x650d30f44220_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x650d30ef8ba0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x650d30ef8ba0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x650d30ef8ba0_0;
    %assign/vec4 v0x650d30f442c0_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x650d30ef8ba0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x650d30f31010;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x650d30f44420_0, 0, 1;
T_1.0 ;
    %delay 10000, 0;
    %load/vec4 v0x650d30f44420_0;
    %inv;
    %store/vec4 v0x650d30f44420_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x650d30f31010;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x650d30f444f0_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x650d30f444f0_0, 0, 1;
    %delay 50000000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x650d30f444f0_0, 0, 1;
    %delay 10000000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x650d30f444f0_0, 0, 1;
    %delay 100000000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x650d30f444f0_0, 0, 1;
    %delay 10000000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x650d30f444f0_0, 0, 1;
    %delay 20000000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x650d30f444f0_0, 0, 1;
    %delay 10000000, 0;
    %delay 100000, 0;
    %vpi_call 2 54 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x650d30f31010;
T_3 ;
    %vpi_call 2 58 "$dumpfile", "ContadorConEcho_tb.vcd" {0 0 0};
    %vpi_call 2 59 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x650d30f31010 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "ContadorConEcho_tb.v";
    "ContadorConEcho.v";
