#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5a7a348ae020 .scope module, "ContadorConTrigger_tb" "ContadorConTrigger_tb" 2 3;
 .timescale -9 -12;
v0x5a7a348bf720_0 .var "clk", 0 0;
v0x5a7a348bf7f0_0 .net "trig", 0 0, v0x5a7a348bf620_0;  1 drivers
S_0x5a7a348ae1b0 .scope module, "uut" "ContadorConTrigger" 2 10, 3 1 0, S_0x5a7a348ae020;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 1 "trig";
P_0x5a7a348ae390 .param/l "TRIG_PULSE_WIDTH" 0 3 6, C4<0111110100>;
v0x5a7a34875ba0_0 .net "clk", 0 0, v0x5a7a348bf720_0;  1 drivers
v0x5a7a348bf540_0 .var "counter", 31 0;
v0x5a7a348bf620_0 .var "trig", 0 0;
E_0x5a7a348748a0 .event posedge, v0x5a7a34875ba0_0;
    .scope S_0x5a7a348ae1b0;
T_0 ;
    %wait E_0x5a7a348748a0;
    %load/vec4 v0x5a7a348bf540_0;
    %cmpi/u 500, 0, 32;
    %jmp/0xz  T_0.0, 5;
    %load/vec4 v0x5a7a348bf540_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x5a7a348bf540_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5a7a348bf620_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5a7a348bf540_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5a7a348bf620_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5a7a348ae020;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a7a348bf720_0, 0, 1;
T_1.0 ;
    %delay 10000, 0;
    %load/vec4 v0x5a7a348bf720_0;
    %inv;
    %store/vec4 v0x5a7a348bf720_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5a7a348ae020;
T_2 ;
    %delay 100000, 0;
    %delay 50000000, 0;
    %delay 100000, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5a7a348ae020;
T_3 ;
    %vpi_call 2 34 "$dumpfile", "ContadorConTrigger_tb.vcd" {0 0 0};
    %vpi_call 2 35 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5a7a348ae020 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "ContadorConTrigger_tb.v";
    "ContadorConTrigger.v";
