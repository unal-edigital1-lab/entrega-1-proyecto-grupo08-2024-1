#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x600b8db3d450 .scope module, "ControlLed_tb" "ControlLed_tb" 2 3;
 .timescale -9 -12;
v0x600b8db51470_0 .net "aux", 0 0, v0x600b8db2ac70_0;  1 drivers
v0x600b8db51540_0 .var "clk", 0 0;
v0x600b8db51610_0 .var "echo_duration", 31 0;
S_0x600b8db3d5e0 .scope module, "UUT" "ControlLed" 2 11, 3 1 0, S_0x600b8db3d450;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 32 "echo_duration";
    .port_info 2 /OUTPUT 1 "aux";
P_0x600b8db275c0 .param/l "DISTANCE_2CM" 0 3 14, C4<00101101010100>;
P_0x600b8db27600 .param/l "DISTANCE_5CM" 0 3 15, C4<11100010100100>;
v0x600b8db2ac70_0 .var "aux", 0 0;
v0x600b8db51270_0 .net "clk", 0 0, v0x600b8db51540_0;  1 drivers
v0x600b8db51330_0 .net "echo_duration", 31 0, v0x600b8db51610_0;  1 drivers
E_0x600b8db3ba20 .event edge, v0x600b8db51270_0;
    .scope S_0x600b8db3d5e0;
T_0 ;
    %wait E_0x600b8db3ba20;
    %pushi/vec4 2900, 0, 32;
    %load/vec4 v0x600b8db51330_0;
    %cmp/u;
    %flag_get/vec4 5;
    %load/vec4 v0x600b8db51330_0;
    %cmpi/u 14500, 0, 32;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x600b8db2ac70_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x600b8db51330_0;
    %cmpi/u 14500, 0, 32;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %flag_mov 8, 5;
    %load/vec4 v0x600b8db51330_0;
    %cmpi/u 2900, 0, 32;
    %flag_or 5, 8;
    %jmp/0xz  T_0.2, 5;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x600b8db2ac70_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x600b8db3d450;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600b8db51540_0, 0, 1;
T_1.0 ;
    %delay 10000, 0;
    %load/vec4 v0x600b8db51540_0;
    %inv;
    %store/vec4 v0x600b8db51540_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x600b8db3d450;
T_2 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x600b8db51610_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 5000, 0, 32;
    %store/vec4 v0x600b8db51610_0, 0, 32;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x600b8db51610_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 2000, 0, 32;
    %store/vec4 v0x600b8db51610_0, 0, 32;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x600b8db51610_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 16000, 0, 32;
    %store/vec4 v0x600b8db51610_0, 0, 32;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x600b8db51610_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 10000, 0, 32;
    %store/vec4 v0x600b8db51610_0, 0, 32;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x600b8db51610_0, 0, 32;
    %delay 10000, 0;
    %vpi_call 2 57 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x600b8db3d450;
T_3 ;
    %vpi_call 2 61 "$dumpfile", "ControlLed_tb.vcd" {0 0 0};
    %vpi_call 2 62 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x600b8db3d450 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "ControlLed_tb.v";
    "ControlLed.v";
