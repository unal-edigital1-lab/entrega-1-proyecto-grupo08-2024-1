:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x60c64fc865b0 .scope module, "ContadorConTrigger_tb" "ContadorConTrigger_tb" 2 3;
 .timescale -9 -12;
v0x60c64fc97c20_0 .var "clk", 0 0;
v0x60c64fc97cf0_0 .net "trigger", 0 0, v0x60c64fc97b20_0;  1 drivers
S_0x60c64fc86740 .scope module, "uut" "ContadorConTrigger" 2 10, 3 1 0, S_0x60c64fc865b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 1 "trigger";
P_0x60c64fc86920 .param/l "limite" 0 3 9, C4<00000000000111110100>;
v0x60c64fc4dba0_0 .net "clk", 0 0, v0x60c64fc97c20_0;  1 drivers
v0x60c64fc97a40_0 .var "contador1", 19 0;
v0x60c64fc97b20_0 .var "trigger", 0 0;
E_0x60c64fc4c7f0 .event posedge, v0x60c64fc4dba0_0;
    .scope S_0x60c64fc86740;
T_0 ;
    %pushi/vec4 0, 0, 20;
    %store/vec4 v0x60c64fc97a40_0, 0, 20;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60c64fc97b20_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x60c64fc86740;
T_1 ;
    %wait E_0x60c64fc4c7f0;
    %load/vec4 v0x60c64fc97a40_0;
    %cmpi/u 500, 0, 20;
    %jmp/0xz  T_1.0, 5;
    %load/vec4 v0x60c64fc97a40_0;
    %addi 1, 0, 20;
    %assign/vec4 v0x60c64fc97a40_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x60c64fc97b20_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60c64fc97b20_0, 0, 1;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x60c64fc865b0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60c64fc97c20_0, 0, 1;
T_2.0 ;
    %delay 10000, 0;
    %load/vec4 v0x60c64fc97c20_0;
    %inv;
    %store/vec4 v0x60c64fc97c20_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x60c64fc865b0;
T_3 ;
    %delay 100000, 0;
    %delay 600000000, 0;
    %delay 100000, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x60c64fc865b0;
T_4 ;
    %vpi_call 2 34 "$dumpfile", "ContadorConTrigger_tb.vcd" {0 0 0};
    %vpi_call 2 35 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x60c64fc865b0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
