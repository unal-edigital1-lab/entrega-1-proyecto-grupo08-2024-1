#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5d0d4d36e150 .scope module, "ContadorConEcho_tb" "ContadorConEcho_tb" 2 3;
 .timescale -9 -12;
v0x5d0d4d3803d0_0 .var "clk", 0 0;
v0x5d0d4d3804a0_0 .net "contador2", 19 0, v0x5d0d4d335ba0_0;  1 drivers
v0x5d0d4d380570_0 .var "echo", 0 0;
S_0x5d0d4d36e2e0 .scope module, "UUT" "ContadorConEcho" 2 11, 3 1 0, S_0x5d0d4d36e150;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "echo";
    .port_info 2 /OUTPUT 20 "contador2";
v0x5d0d4d3357b0_0 .net "clk", 0 0, v0x5d0d4d3803d0_0;  1 drivers
v0x5d0d4d335ba0_0 .var "contador2", 19 0;
v0x5d0d4d3802b0_0 .net "echo", 0 0, v0x5d0d4d380570_0;  1 drivers
E_0x5d0d4d3341f0 .event posedge, v0x5d0d4d3357b0_0;
    .scope S_0x5d0d4d36e2e0;
T_0 ;
    %pushi/vec4 0, 0, 20;
    %store/vec4 v0x5d0d4d335ba0_0, 0, 20;
    %end;
    .thread T_0;
    .scope S_0x5d0d4d36e2e0;
T_1 ;
    %wait E_0x5d0d4d3341f0;
    %load/vec4 v0x5d0d4d3802b0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %load/vec4 v0x5d0d4d335ba0_0;
    %addi 1, 0, 20;
    %store/vec4 v0x5d0d4d335ba0_0, 0, 20;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 0, 0, 20;
    %store/vec4 v0x5d0d4d335ba0_0, 0, 20;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5d0d4d36e150;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d0d4d3803d0_0, 0, 1;
T_2.0 ;
    %delay 10000, 0;
    %load/vec4 v0x5d0d4d3803d0_0;
    %inv;
    %store/vec4 v0x5d0d4d3803d0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5d0d4d36e150;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d0d4d380570_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d0d4d380570_0, 0, 1;
    %delay 600000000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d0d4d380570_0, 0, 1;
    %delay 200000, 0;
    %delay 100000, 0;
    %vpi_call 2 40 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5d0d4d36e150;
T_4 ;
    %vpi_call 2 44 "$dumpfile", "ContadorConEcho_tb.vcd" {0 0 0};
    %vpi_call 2 45 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5d0d4d36e150 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "ContadorConEcho_tb.v";
    "ContadorConEcho.v";
